<h2>This is a listing of all the quotes</h2>

<% @quotes.each do |quote| %>
  <h3><%= quote.quote %></h3>
  <h5><%= quote.source %></h5>
  <%= link_to " Go to Quote", quote_path(quote) %>
  <%= link_to " Edit", edit_quote_path(quote) %>
  <%= link_to " Delete", quote_path(quote), method: :delete, data: {confirm: "Are you sure you want to delete "} %>
<% end %>
